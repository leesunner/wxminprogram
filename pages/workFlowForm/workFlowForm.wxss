/* pages/workFlowForm/workFlowForm.wxss */
.content{
  font-size: 30rpx;
  padding: 35rpx 0;
  padding-bottom: 100rpx;
}
.list{
  margin-bottom: 38rpx;
  padding: 0 30rpx;
}
.label{
  margin-bottom: 23rpx;
}
text{
  vertical-align: middle;
}
input,textarea{
  display: block;
  border:solid 1rpx #B3B3B3;
  padding-left: 26rpx;
  height: 80rpx;
  width: auto;
  color: #666666;
}
label,image{
  vertical-align: middle;
  display: inline-block;
  color: #666666;
}
.file{
  min-width: 280rpx;
  height: 100rpx;
  color: #FF8000;
  text-align: center;
  line-height: 100rpx;
  background-color: #EDEDED;
}
.icon{
  width: 46rpx;
  height: 46rpx;
  display: inline-block;
  margin-right: 10rpx;
  margin-left: 120rpx;
}
textarea{
  height: 250rpx;
  padding-top: 26rpx;
}
.star{
  color: #E42419;
  font-size: 24rpx;
}
.showImage{
  width: 200rpx;
  height: 200rpx;
  margin-right: 4rpx;
}
.buttons{
  position: fixed;
  z-index: 100;
  bottom:0;
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  left: 0;
}
.buttons button{
  height: 90rpx;
  color: #fff;
  font-size: 30rpx;
  border:none;
  width: 50%;
  border-radius: 0;
  padding: 0 0;
  line-height: 90rpx;
}
.buttons button:nth-child(1){
  background-color: #E42419;
}
.buttons button:nth-child(2){
  background-color: #FF8000;
}
.buttons .obtn{
  width: 100%;
}
.select{
  border: solid 1rpx #B3B3B3;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 26rpx;
}
.load{
  font-size: 35rpx;
  color: #E42419;
  margin: 24rpx 24rpx;
}
/* 对话框 */
.dkDralog{
  position: fixed;
  z-index: 999;
  min-height: 100%;
  height: 100%;
  width: 100%;
  background-color: rgba(0,0,0,.25);
  top: 0;
  left: 0;
}
.dkDralog_box{
  margin-top: 75%;
  width: 85%;
  margin-left: auto;
  margin-right: auto;
  border-radius: 12rpx;
  background-color: #ffffff;
  text-align: center;
}
.dkDralog_box_title{
  font-size: 32rpx;
  height: 45px;
  line-height: 45px;
  color: #000000;
}
.dkDralog_con{
  color: #666666;
  font-size: 28rpx;
  padding: 16px 30px;
}
.dkDralog_btn{
  display: flex;
  height: 42px;
  align-items: center;
  justify-content: space-between;
  flex-flow: nowrap row;
  border-top: solid 1rpx #EDEDED;
}
.dkDralog_btn button{
  width: 50%;
  height: 42px;
  font-size: 30rpx;
  border:none;
  border-radius: 0;
  padding: 0 0;
  line-height: 42px;
  color: #000000;
}
.dkDralog_btn .dkDralog_btn_two{
  border-left: solid 1rpx #EDEDED;
  color: #E42419;
}
