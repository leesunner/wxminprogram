/* pages/mine/feedback/feedback.wxss */
.content{
  font-size: 30rpx;
  color: #666666;
}
.vont{
  padding: 30rpx 30rpx;
}
.label{
  margin-bottom: 46rpx;
}
.label:nth-child(2){
  padding-bottom: 40rpx;
  border-bottom: solid 1rpx #E6E6E6;
}
.tab{
  margin-bottom: 20rpx;
}
.imgup{
  width: 200rpx;
  height: 200rpx;
  color: #B3B3B3;
  text-align: center;
  background-color: #EDEDED;
}
.imgup image{
  width: 52rpx;
  height: 52rpx;
  margin-top: 40rpx;
  margin-bottom: 27rpx;
  margin-right: 10rpx;
  display: inline-block;
}
input,textarea{
  border:solid 1rpx #B3B3B3;
  display: block;
  height: 80rpx;
  width: auto;
}
input{
  padding-left: 26rpx;
}
textarea{
  height:250rpx;
  padding:19rpx 26rpx;
}
.btn{
  position: fixed;
  z-index: 100;
  bottom:0;
  width: 100%;
}
.submit{
  height: 90rpx;
  color: #fff;
  font-size: 30rpx;
  border:none;
  border-radius: 0;
  padding: 0 0;
  line-height: 90rpx;
  background-color: #FF8000;
}
.btn button{
  min-width: 100%;
}
.gay{
  color: #E6E6E6;
}