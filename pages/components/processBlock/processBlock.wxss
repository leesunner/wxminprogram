/* pages/components/processBlock/processBlock.wxss */
.processBox{
  padding-left: 36rpx;
  padding-bottom: 36rpx;
  position: relative;
}
.processBox_left{
  position: absolute;
  top: 0;
  left: 20rpx;
  height: 100%;
}
.processBox_left::before{
  content: '';
  position: absolute;
  border-right: solid 1px  #B3B3B3;
  top: 20rpx;
  left: 10rpx;
  height: 100%;
  width: 4rpx;
}
.processBox_leftd::before{
  height: 60%;
}
.processBox_dot{
  width: 24rpx;
  height: 24rpx;
  margin-top:6rpx;
  border-radius: 50%;
  background-color: #B3B3B3;
}
.processBox_right{
  margin-left: 34rpx;
}
.processBox_box{
  margin-top: 10rpx;
  border-right: 12rpx;
  padding:30rpx 30rpx;
  box-shadow: 0 0 15rpx 1rpx rgba(10,16,20,.12);
  position: relative;
}
.processBox_item{
  font-size: 30rpx;
  color: #666666;
}
.processBox_box_btn{
  position: absolute;
  bottom: 30rpx;
  right: 30rpx;
  padding: 4rpx 28rpx;
  border: solid 1rpx #FF8800;
  border-radius: 8rpx;
}
.processBox_box_btn text{
  vertical-align: middle;
  color: #FF8800;
  font-size: 30rpx;
}
.processBox_box_btn image{
  display: inline-block;
  vertical-align: middle;
  height: 26rpx;
  width: 30.8rpx;
  margin-right: 12rpx;
}